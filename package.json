{
  "name": "@erozak/eslint-config",
  "version": "3.8.1",
  "description": "",
  "main": "js-config.js",
  "exports": {
    ".": "./js-config.js",
    "./typescript": "./ts-config.js"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint:ci": "eslint",
    "lint:commit": "commitlint",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "test": "node ./tests/test.js",
    "semantic-release": "npx semantic-release",
    "commit": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erozak/eslint-config.git"
  },
  "author": "erozak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erozak/eslint-config/issues"
  },
  "homepage": "https://github.com/erozak/eslint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/cz-commitlint": "^17.0.0",
    "@commitlint/format": "^17.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "assert": "^2.0.0",
    "commitizen": "^4.2.4",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "lint-staged": "^13.0.0",
    "pinst": "^3.0.0",
    "semantic-release": "^19.0.2",
    "typescript": "^4.7.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "packageManager": "pnpm@7.2.1",
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.26.0"
  }
}
