{
  "name": "@erozak/eslint-config",
  "version": "3.7.0",
  "description": "",
  "main": "js-config.js",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "lint:ci": "eslint",
    "lint:commit": "commitlint",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "test": "node ./tests/test.js",
    "semantic-release": "semantic-release",
    "commit": "cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erozak/eslint-config.git"
  },
  "author": "erozak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erozak/eslint-config/issues"
  },
  "homepage": "https://github.com/erozak/eslint-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/cz-commitlint": "^16.0.0",
    "@commitlint/format": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "assert": "^2.0.0",
    "commitizen": "^4.2.4",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "pinst": "^2.1.6",
    "semantic-release": "^18.0.1",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "packageManager": "yarn@3.1.1",
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^40.0.0"
  }
}
